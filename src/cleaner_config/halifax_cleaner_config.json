                }
            },
            "money_in": {
                "processor": "clean_currency",
                "options": {
                    "decimal_places": 2,
                    "remove_symbols": ["£", "$", "€"]
                }
            },
            "start_balance": {
                "processor": "clean_currency",
                "options": {
                    "decimal_places": 2,
                    "remove_symbols": ["£", "$", "€"]
                }
            }
        },
        "tables": {
            "data": {
                "date": {
                    "processor": "clean_date",
                    "options": {
                        "input_format": "%d %b %y",
